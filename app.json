{
  "expo": {
    "name": "Milkey",
    "slug": "milkey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/milkey-logo.png",
    "scheme": "milkey",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/milkey-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D4A6B"
    },
    "android": {
      "package": "com.milkey.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/milkey-logo.png",
        "backgroundColor": "#0D4A6B"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/milkey-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f26dd802-1e7e-47ba-af3f-2db9acd17637"
      }
    }
  }
}
